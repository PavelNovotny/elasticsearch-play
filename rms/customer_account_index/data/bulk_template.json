{{#repeatWithId  1 'R1' comma=false}}

  {"index":{"_id":"CARN{{@index_R1}}_{{offset}}","_type":"customer_account","_index":"customer_account_index_rif"}}

  {
    "customerRequests": [{{#repeat 1 3}}{"crId": "crId{{int 100000000 999999999}}"}{{/repeat}}],
    "deliveryAddress": [
      {{#repeat 1 3}}
        {
          "buildingNo": "{{int 1 2000}}",
          "city": "{{city}}",
          "cityDistrict": "{{city}}",
          "countryCode": "{{countryCode}}",
          "crossId": "{{email}}",
          "postalCode": "{{zipcode}}",
          "street": "{{street}}",
          "streetNo": "{{int 1 2000}}"
        }
      {{/repeat}}
    ],
    "deliveryEmail": [{{#repeat 1 3}}{"email":"{{email}}"}{{/repeat}}],
    "individualOffers": [{{#repeat 1 3}}{"inId": "{{int 100000 999999}}"}{{/repeat}}],
    "invoices": [{{#repeat 1 3}}{"variableSymbol": "{{int 100000 999999}}"}{{/repeat}}],
    "orders": [{{#repeat 1 3}}{"orderId": "{{int 100000 999999}}"}{{/repeat}}],
    "billingPeriod": "{{int 20  30 }}",
    "birthDate": "{{date "1900" "2000" "DD-MM-YYYY"}}",
    "birthNumber": "{{date "1900" "2000" "DDMMYYYYYYYY"}}",
    "caRefNo":"CARN{{@index_R1}}_{{offset}}",
    "cuRefNo":"CRN{{@index_R1}}_{{offset}}",
    "name": "{{firstName}} {{lastName}}"
  }


  {"index":{"_id":"{{@index_R1}}_{{offset}}","_type":"customer_account_permission","_index":"customer_account_index_rif","_parent":"CARN{{@index_R1}}_{{offset}}"}}

  {"login":"x0503{{int 100  999 }}"}

  {{#repeat 10 comma=false}}

    {"index":{"_id":"I{{@index_R1}}_{{offset}}{{@index}}","_type":"invoice_type","_index":"customer_account_index_rif","_parent":"CARN{{@index_R1}}_{{offset}}"}}

    {"variableSymbol": "{{int 100000 999999}}"}

  {{/repeat}}

  {"index":{"_id":"{{@index_R1}}_{{offset}}","_type":"product","_index":"customer_account_index_rif","_parent":"CARN{{@index_R1}}_{{offset}}"}}

  {
    "imsi": "{{int 100000000 999999999}}",
    "phoneNumber": "{{phone 'xxxxxxxxx'}}",
    "serialNumber": "{{int 100000000 999999999}}",
    "serviceId": "{{int 100000000 999999999}}",
    "installationAddress": [
        {{#repeat 1 3}}
          {
            "buildingNo": "{{int 1 2000}}",
            "city": "{{city}}",
            "cityDistrict": "{{city}}",
            "countryCode": "{{countryCode}}",
            "crossId": "{{email}}",
            "postalCode": "{{zipcode}}",
            "street": "{{street}}",
            "streetNo": "{{int 1 2000}}"
          }
        {{/repeat}}
      ]
  }

{{/repeatWithId}}